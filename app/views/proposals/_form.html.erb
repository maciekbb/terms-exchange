<%= simple_form_for @proposal do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :term_id, collection: Subject.all, as: :grouped_select, group_method: :terms %>
  	<%= f.input :preferred %>
  	<%= f.input :reason %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
